{
  "name": "cripto-change",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "prettier/react"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@types/react-dom": "^17.0.11",
    "@types/react-modal": "^3.13.1",
    "ag-grid-community": "^26.2.1",
    "ag-grid-react": "^26.2.0",
    "axios": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-superjson-next": "^0.4.3",
    "framer-motion": "^6.2.7",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-charts": "^4.0.0",
    "react-hook-form": "^7.27.1",
    "react-loading-skeleton": "^3.1.0",
    "react-modal": "^3.14.4",
    "sass": "^1.49.0",
    "superjson": "^1.8.1",
    "use-context-selector": "^1.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "eslint": "^8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "miragejs": "^0.1.43",
    "prettier": "2.6.2",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint",
      "prettier --write"
    ]
  },
  "overrides": [
    {
      "files": "['*.ts', '*.tsx']",
      "rules": {
        "no-undef": 0
      }
    }
  ]
}
